RewriteEngine On
RewriteBase /

# Excluir archivos estáticos de las reglas de rewrite
RewriteCond %{REQUEST_FILENAME} \.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|map)$ [NC,OR]
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^ - [L]

RewriteCond %{SCRIPT_FILENAME} !-d
RewriteCond %{SCRIPT_FILENAME} !-f


RewriteRule ^s/([^/]*)/$ set/$1.php?lang=es [L,QSA]
RewriteRule ^g/([^/]*)/$ get/$1.php?lang=es [L,QSA]

# Rutas de miembros (login/registro)
RewriteRule ^miembros/?$ miembros/index.php [L,QSA]
RewriteRule ^miembros/crear-cuenta/?$ miembros/register.php [L,QSA]
RewriteRule ^miembros/recuperar-contrasena/?$ miembros/forgot_pass.php [L,QSA]
RewriteRule ^miembros/cerrar-sesion/?$ miembros/cerrar-sesion.php [L,QSA]
RewriteRule ^miembros/s/([^/]*)/?$ miembros/set/$1.php?lang=es [L,QSA]
RewriteRule ^miembros/g/([^/]*)/?$ miembros/get/$1.php?lang=es [L,QSA]

